"parent_id": "24",
"tags": PackedStringArray(),
"text": "Did you really steal your sister's life with no remorse, no regret?",
"text_replacements": Array[Dictionary]([]),
"translation_key": "Did you really steal your sister's life with no remorse, no regret?",
"type": "response"
},
"3": {
