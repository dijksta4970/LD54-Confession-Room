"id": "23",
"next_id": "24",
"parent_id": "22",
"tags": PackedStringArray(),
"text": "也许是我想错了，也许她是为了日渐衰老的父母才选择留下的。",
"text_replacements": Array[Dictionary]([]),
"translation_key": "也许是我想错了，也许她是为了日渐衰老的父母才选择留下的。",
"type": "dialogue"
},
