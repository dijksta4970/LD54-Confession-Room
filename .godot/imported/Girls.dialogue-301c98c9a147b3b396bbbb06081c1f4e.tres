"id": "21",
"next_id": "24",
"parent_id": "20",
"tags": PackedStringArray(),
"text": "也许真的是这样，也许她更喜欢村子里的生活。",
"text_replacements": Array[Dictionary]([]),
"translation_key": "也许真的是这样，也许她更喜欢村子里的生活。",
"type": "dialogue"
},
